{
  "name": "nativescript-ably",
  "version": "0.2.0",
  "description": "A wrapper for ably.io realtime comunication system",
  "main": "ably.js",
  "typings": "ably.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.0.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "copyfiles": "cp -rf package.json dist && cp -rf README.md dist && cp -rf LICENSE dist",
    "dist": "npm run build && npm run copyfiles && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atende/nativescript-ably.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS",
    "Realtime",
    "PubSub",
    "Ably"
  ],
  "author": {
    "name": "Giovanni",
    "email": "giovanni@atende.info"
  },
  "bugs": {
    "url": "https://github.com/atende/nativescript-ably/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/atende/nativescript-ably/blob/master/LICENSE"
  },
  "homepage": "https://github.com/atende/nativescript-ably",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "tns-core-modules": "^2.3.0",
    "tns-platform-declarations": "^2.3.0",
    "typescript": "^2.0.7"
  },
  "peerDependencies": {
    "rxjs": ">=5.0.0-beta.12"
  }
}
